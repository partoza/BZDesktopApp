@page "/admin/dashboard"

<div class="grid grid-cols-1 gap-3 xl:grid-cols-12">

    <!-- LEFT COLUMN (col-span-8) -->
    <div class="flex flex-col gap-3 xl:col-span-7">
        <!-- Welcome Card -->
        <div class="flex h-44 items-center justify-between rounded-xl bg-white p-6 shadow-sm">
            <div>
                <h1 class="text-xl font-semibold text-gray-800">
                    Hello <span class="text-primary">John Rex!</span>, Welcome to BZ
                </h1>
                <p class="text-md mt-1 font-medium">Ready to make some sales today?</p>

                <div class="mt-4 flex items-center gap-2 rounded-lg bg-[#F1C40F]/10 p-2 text-xs text-[#F1C40F]">
                    <span>
                        <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="currentColor" class="size-4">
                            <path d="M5.85 3.5a.75.75 0 0 0-1.117-1 9.719 9.719 0 0 0-2.348 4.876.75.75 0 0 0 1.479.248A8.219 8.219 0 0 1 5.85 3.5ZM19.267 2.5a.75.75 0 1 0-1.118 1 8.22 8.22 0 0 1 1.987 4.124.75.75 0 0 0 1.48-.248A9.72 9.72 0 0 0 19.266 2.5Z" />
                            <path fill-rule="evenodd" d="M12 2.25A6.75 6.75 0 0 0 5.25 9v.75a8.217 8.217 0 0 1-2.119 5.52.75.75 0 0 0 .298 1.206c1.544.57 3.16.99 4.831 1.243a3.75 3.75 0 1 0 7.48 0 24.583 24.583 0 0 0 4.83-1.244.75.75 0 0 0 .298-1.205 8.217 8.217 0 0 1-2.118-5.52V9A6.75 6.75 0 0 0 12 2.25ZM9.75 18c0-.034 0-.067.002-.1a25.05 25.05 0 0 0 4.496 0l.002.1a2.25 2.25 0 1 1-4.5 0Z" clip-rule="evenodd" />
                        </svg>
                    </span>
                    <span>There are <span class="font-semibold">2 products</span> low in stock. <a href="#" class="text- font-medium underline">View products here</a>.</span>
                </div>
            </div>

            <img src="images/background/dashboard-illustration.png"
                 class="h-40 w-52 object-contain" />
        </div>


        <!-- Analytics Section -->
        <div class="h-(400px) rounded-xl bg-white p-6 shadow-sm">
            <h2 class="mb-3 text-lg font-medium text-gray-800">Analytics by Product Categories</h2>

            <div class="grid grid-cols-1 gap-6 lg:grid-cols-2">
                <!-- Donut Chart Section -->
                <div class="flex flex-col items-center justify-center">
                    <div class="relative mb-6 h-64 w-64">
                        <!-- Outer ring -->
                        <div class="absolute inset-0 rounded-full bg-gray-100"></div>

                        <!-- Donut chart -->
                        <div class="absolute inset-2 rounded-full"
                             style="background: conic-gradient(#2F7D6D 0% 50%, #7EAFA5 50% 85%, #C6D8D5 85% 100%);">
                        </div>

                        <!-- Inner circle -->
                        <div class="absolute inset-8 rounded-full bg-white shadow-sm"></div>

                        <!-- Center text -->
                        <div class="absolute inset-0 flex flex-col items-center justify-center">
                            <span class="text-2xl font-bold text-gray-800">₱500K</span>
                            <span class="text-sm text-gray-500">Total Sales</span>
                        </div>

                        <!-- Percentage labels -->
                        <div class="absolute -top-2 left-1/2 -translate-x-1/2 transform rounded-lg bg-white px-3 py-2 text-center shadow-md">
                            <span class="text-sm font-bold text-[#2F7D6D]">15.0%</span>
                            <div class="mt-1 text-xs text-gray-500">Solar</div>
                        </div>
                        <div class="absolute -right-2 bottom-16 rounded-lg bg-white px-3 py-2 text-center shadow-md">
                            
                            <span class="text-sm font-bold text-[#2F7D6D]">50.0%</span>
                            <div class="mt-1 text-xs text-gray-500">Computer Parts</div>
                        </div>
                        <div class="absolute bottom-16 -left-2 rounded-lg bg-white px-3 py-2 text-center shadow-md">
                            <span class="text-sm font-bold text-[#2F7D6D]">35.0%</span>
                            <div class="mt-1 text-xs text-gray-500">CCTV</div>
                        </div>
                    </div>

                    <!-- Legend -->
                    <div class="flex justify-center space-x-6">
                        <div class="flex items-center">
                            <div class="mr-2 h-3 w-3 rounded-full bg-[#2F7D6D]"></div>
                            <span class="text-sm text-gray-600">Computer Parts</span>
                        </div>
                        <div class="flex items-center">
                            <div class="mr-2 h-3 w-3 rounded-full bg-[#7EAFA5]"></div>
                            <span class="text-sm text-gray-600">CCTV</span>
                        </div>
                        <div class="flex items-center">
                            <div class="mr-2 h-3 w-3 rounded-full bg-[#C6D8D5]"></div>
                            <span class="text-sm text-gray-600">Solar</span>
                        </div>
                    </div>
                </div>

                <!-- Category Details -->
                <div class="rounded-xl bg-white px-5 py-2 shadow-sm">
                    <!-- Header -->
                    <div class="mb-2 flex items-center justify-between">
                        <h3 class="text-sm font-semibold text-gray-800">Product Categories</h3>
                        <div class="flex items-center gap-2 text-sm text-gray-500">
                            <div class="h-2 w-2 rounded-full bg-blue-500"></div>
                            <span class="text-xs">Performance</span>
                        </div>
                    </div>

                    <!-- Category Cards -->
                    <div class="space-y-3">
                        <!-- Computer Parts -->
                        <div class="flex gap-3 rounded-lg border border-gray-100 bg-white p-3 shadow-sm transition-all duration-300 hover:shadow">
                            <div class="flex h-10 w-10 items-center justify-center rounded-lg bg-emerald-50">
                                <img src="https://cdn-icons-png.flaticon.com/512/1055/1055687.png"
                                     class="h-5 w-5 object-contain" />
                            </div>
                            <div class="flex-1">
                                <div class="mb-1 flex items-start justify-between">
                                    <h3 class="text-sm font-semibold text-gray-900">Computer Parts</h3>
                                    <span class="rounded bg-emerald-50 px-2 py-1 text-xs font-semibold text-gray-900">₱250,000</span>
                                </div>
                                <div class="flex items-center gap-3">
                                    <div class="flex items-center text-xs font-medium text-emerald-600">
                                        <svg class="mr-1 h-3 w-3" fill="currentColor" viewBox="0 0 20 20">
                                            <path fill-rule="evenodd" d="M5.293 9.707a1 1 0 010-1.414l4-4a1 1 0 011.414 0l4 4a1 1 0 01-1.414 1.414L11 7.414V15a1 1 0 11-2 0V7.414L6.707 9.707a1 1 0 01-1.414 0z" clip-rule="evenodd"></path>
                                        </svg>
                                        +20%
                                    </div>
                                    <p class="text-xs text-gray-500">Intel i7 Processor</p>
                                </div>
                            </div>
                        </div>

                        <!-- CCTV -->
                        <div class="flex gap-3 rounded-lg border border-gray-100 bg-white p-3 shadow-sm transition-all duration-300 hover:shadow">
                            <div class="flex h-10 w-10 items-center justify-center rounded-lg bg-blue-50">
                                <img src="https://cdn-icons-png.flaticon.com/512/483/483947.png"
                                     class="h-5 w-5 object-contain" />
                            </div>
                            <div class="flex-1">
                                <div class="mb-1 flex items-start justify-between">
                                    <h3 class="text-sm font-semibold text-gray-900">CCTV</h3>
                                    <span class="rounded bg-blue-50 px-2 py-1 text-xs font-semibold text-gray-900">₱175,000</span>
                                </div>
                                <div class="flex items-center gap-3">
                                    <div class="flex items-center text-xs font-medium text-emerald-600">
                                        <svg class="mr-1 h-3 w-3" fill="currentColor" viewBox="0 0 20 20">
                                            <path fill-rule="evenodd" d="M5.293 9.707a1 1 0 010-1.414l4-4a1 1 0 011.414 0l4 4a1 1 0 01-1.414 1.414L11 7.414V15a1 1 0 11-2 0V7.414L6.707 9.707a1 1 0 01-1.414 0z" clip-rule="evenodd"></path>
                                        </svg>
                                        +15%
                                    </div>
                                    <p class="text-xs text-gray-500">HikVision Dome Camera</p>
                                </div>
                            </div>
                        </div>

                        <!-- Solar -->
                        <div class="flex gap-3 rounded-lg border border-gray-100 bg-white p-3 shadow-sm transition-all duration-300 hover:shadow">
                            <div class="flex h-10 w-10 items-center justify-center rounded-lg bg-amber-50">
                                <img src="https://cdn-icons-png.flaticon.com/512/1698/1698535.png"
                                     class="h-5 w-5 object-contain" />
                            </div>
                            <div class="flex-1">
                                <div class="mb-1 flex items-start justify-between">
                                    <h3 class="text-sm font-semibold text-gray-900">Solar</h3>
                                    <span class="rounded bg-amber-50 px-2 py-1 text-xs font-semibold text-gray-900">₱75,000</span>
                                </div>
                                <div class="flex items-center gap-3">
                                    <div class="flex items-center text-xs font-medium text-red-600">
                                        <svg class="mr-1 h-3 w-3" fill="currentColor" viewBox="0 0 20 20">
                                            <path fill-rule="evenodd" d="M14.707 10.293a1 1 0 010 1.414l-4 4a1 1 0 01-1.414 0l-4-4a1 1 0 111.414-1.414L9 12.586V5a1 1 0 012 0v7.586l2.293-2.293a1 1 0 011.414 0z" clip-rule="evenodd"></path>
                                        </svg>
                                        -10%
                                    </div>
                                    <p class="text-xs text-gray-500">100W Solar Panel</p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <!-- RIGHT COLUMN (col-span-4) -->
